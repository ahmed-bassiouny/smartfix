<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="smartfixsa.com.smartfix">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".acitivities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".acitivities.RegisterStuffActivity" />
        <activity android:name=".acitivities.MobileCompanyActivity" >
        </activity>
        <activity android:name=".acitivities.MaintenanceRequestAppleActivity" />
        <activity android:name=".acitivities.MaintenanceRequestOtherActivity" />
        <activity android:name=".acitivities.StoreListActivity" />
        <activity android:name=".acitivities.MaintenanceCentersListActivity" />
        <activity android:name=".acitivities.MaintenanceCenterDetailsActivity" />
        <activity android:name=".acitivities.SplashActivity">

        </activity>
    </application>

</manifest>